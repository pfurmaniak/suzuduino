Adafruit_SSD1306 gearDisplay(128, 64, &Wire, -1);
Adafruit_SSD1306 tempDisplay(128, 32, &Wire, -1);
uint8_t currentTemp = 0;

void setup() {
  Wire.begin();
  Serial.begin(115200);
  Serial.println("\nOLED test begin");

  // initDisplay(&gearDisplay);
  // gearDisplay.clearDisplay();
  // gearDisplay.drawBitmap(0, 0, gearFont[1], 128, 64, 1);
  // gearDisplay.display();

  initDisplay(&tempDisplay);
}

void initDisplay(Adafruit_SSD1306* display) {
  if (!display->begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDR)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;);
  }
  delay(2000);
}

void printTemp(Adafruit_SSD1306* display, uint8_t temp) {
  display->clearDisplay();
  char tempStr[3];
  sprintf(tempStr, "%3d", temp);
  for (uint8_t i = 0; i < 3; i++) {
    Serial.print(tempStr[i]);
    switch (tempStr[i]) {
      case ' ':
        display->drawBitmap(i*24, 0, tempFont[12], 24, 32, 1);
        break;
      case '0':
        display->drawBitmap(i*24, 0, tempFont[0], 24, 32, 1);
        break;
      case '1':
        display->drawBitmap(i*24, 0, tempFont[1], 24, 32, 1);
        break;
      case '2':
        display->drawBitmap(i*24, 0, tempFont[2], 24, 32, 1);
        break;
      case '3':
        display->drawBitmap(i*24, 0, tempFont[3], 24, 32, 1);
        break;
      case '4':
        display->drawBitmap(i*24, 0, tempFont[4], 24, 32, 1);
        break;
      case '5':
        display->drawBitmap(i*24, 0, tempFont[5], 24, 32, 1);
        break;
      case '6':
        display->drawBitmap(i*24, 0, tempFont[6], 24, 32, 1);
        break;
      case '7':
        display->drawBitmap(i*24, 0, tempFont[7], 24, 32, 1);
        break;
      case '8':
        display->drawBitmap(i*24, 0, tempFont[8], 24, 32, 1);
        break;
      case '9':
        display->drawBitmap(i*24, 0, tempFont[9], 24, 32, 1);
        break;
    }
  }
  display->drawBitmap(3*24, 0, tempFont[10], 24, 32, 1);
  display->drawBitmap(4*24, 0, tempFont[11], 24, 32, 1);
  display->display();
  Serial.println();
}

void loop() {
  printTemp(&tempDisplay, currentTemp);
  currentTemp = currentTemp < 140 ? currentTemp + 1 : 0;
  Serial.println(currentTemp);
  delay(500);
}
